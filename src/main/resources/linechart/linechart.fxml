<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="linechart.LineChartController">
   <children>
      <LineChart fx:id="myLie" layoutX="25.0" layoutY="25.0" title="LineChart">
        <xAxis>
          <CategoryAxis label="X Axis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Y Axis" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="btn1" layoutX="148.0" layoutY="477.0" mnemonicParsing="false" onAction="#btnAction" text="Load LineChart" />
      <Label fx:id="myLabel" layoutX="291.0" layoutY="444.0" prefHeight="86.0" prefWidth="175.0" textFill="#685dbc">
         <font>
            <Font name="MS Gothic" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
